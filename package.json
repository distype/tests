  },
  "dependencies": {
    "@br88c/node-utils": "^1.1.3",
    "@distype/cmd": "github:distype/cmd#build",
    "distype": "github:distype/distype#build",
    "dotenv": "^16.0.0"
  },
  "devDependencies": {
    "@br88c/eslint-config": "^1.4.0",
    "@types/node": "^16.11.27",
    "eslint": "^8.13.0",
    "typescript": "^4.6.3"
